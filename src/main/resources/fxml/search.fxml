<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml">

    <TextField fx:id="textField" prefWidth="800"/>

    <ScrollPane prefHeight="330">
        <TableView fx:id="table">
            <columns>
                <TableColumn prefWidth="200" text="Word" fx:id="wordColumn"/>
                <TableColumn prefWidth="260" text="Translation" fx:id="translationColumn"/>
                <TableColumn prefWidth="66" text="Level" fx:id="levelColumn"/>
                <TableColumn prefWidth="250" text="List name" fx:id="listNameColumn"/>
            </columns>
        </TableView>
    </ScrollPane>

</VBox>
