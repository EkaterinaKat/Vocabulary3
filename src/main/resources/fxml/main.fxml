<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml">

    <stylesheets>
        <URL value="@/css/general_style.css"/>
        <URL value="@/css/learning_window_style.css"/>
        <URL value="@/css/table_style.css"/>
    </stylesheets>

    <VBox>
        <HBox>
            <ImageView fx:id="logoImageView" fitHeight="300" fitWidth="300"/>
            <Pane fx:id="cataloguePane"
                  maxWidth="500" minWidth="500" maxHeight="500" minHeight="500"/>
        </HBox>
        <Pane fx:id="searchPane" styleClass="black_border" maxWidth="800" minWidth="800" maxHeight="400"
              minHeight="400"/>
    </VBox>

    <Pane fx:id="listPane" styleClass="black_border"/>

</HBox>
